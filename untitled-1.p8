function hit(x,y,w,h)
  collide=false
  for i=x,x+w,w do
    if fget(mget((i+levelxhitvalue)/8,(y+levelyhitvalue)/8))==1 or
         fget(mget(i+levelxhitvalue)/8,(y+levelyhitvalue)/8)==1 then
          collide=true
    end
  end
  for i=y,y+h,h do
    if fget(mget((x+levelxhitvalue)/8,(i+levelyhitvalue)/8))==1 or
         fget(mget((x+levelxhitvalue+w)/8,(i+levelyhitvalue)/8))==1 then
          collide=true
    end
  end
  return collide
end
function shit(x,y,w,h)
  spikecollide=false
  for i=x,x+w,w do
    if fget(mget((i+levelxhitvalue)/8,(y+levelyhitvalue)/8))==2 or
         fget(mget(i+levelxhitvalue)/8,(y+levelyhitvalue)/8)==2 then
          spikecollide=true
    end
  end
  for i=y,y+h,h do
    if fget(mget((x+levelxhitvalue)/8,(i+levelyhitvalue)/8))==2 or
         fget(mget((x+levelxhitvalue+w)/8,(i+levelyhitvalue)/8))==2 then
          spikecollide=true
    end
  end
  return spikecollide
end
function movementvalues()
 xaccel=0.10 --the accel
 slip=0.90 --keep under 1; .9-.95 is kinda good
 maxaccel=0.90
 heartsloseperhitpersecond=1
 hittimermax=30 --how much time is between damage deals in a continous manner
end
function playermovement()
 movementvalues()
 dy=dy+0.1
 if dx>maxaccel then
  dx=maxaccel
 end
 if dx<-maxaccel then
  dx=-maxaccel
 end

  if btn(0) then
    dx=dx-xaccel
  end

  if btn(1) then
    dx=dx+xaccel
  end

  if btnp(2) and
    (grounded) then
    grounded=false
    jump+=1
    dy=dy-1.75
  end
  if btnp(4) and
    (grounded) then
    grounded=false
    jump+=1
    dy=dy-1.5
    hittimer=0
  end

  if hit(x+dx,y,7,7) then
    dx=0
  end

  if hit(x,y+dy,7,7) then
    if dy>0 then
      grounded=true
      hittimer=0
    end
    dy=0
  end
  if shit(x+dx,y,7,7) then
    dx=0
  end

  if shit(x,y+dy,7,7) then
    if dy>0 then
      grounded=true
      if hittimer==0 then
       lives-=heartsloseperhitpersecond
       hittimer=1
       shake+=1
      end
      if hittimer>0 then
       hittimer+=1
      end
      if hittimer>hittimermax then
       hittimer=0
      end
    end
    dy=0
  end

  if grounded then
    dx=dx*slip
    jump=0
  end

  y=y+dy
  x=x+dx
 end
