function _draw()
 doshake()
 cls()
 if selectplayer==1 then -- if the trigger is on, starts the player select screen.
		fselectplayer()
	end
 if game==1 then

  death()
  flives()
  fcoins()

  if level1==1 then
   flevel1()
  end
  if level2==1 then
   flevel2()
  end
  flives()
  fcoins()
  spr(player,x,y)
 end
 if endgame==1 then
  fmnendgame()
  if endduedeath==1 then
   badend()
  end
  if endduewin==1 then
   goodend()
  end
 end
end

